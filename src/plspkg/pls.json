{
  "version": 1633338269,
  "tasks": {
    "go": {
      "$syntax": "go <target>",
      "$description": "Goes to the folder identified by <target>.",
      "$os:windows+folder": {
        "cmd": "call cmd /k cd \"{{folder}}\""
      },
      "$os:linux+folder": {
        "cmd": "cd \"{{folder}}\" && bash --login"
      },
      "$os:macos+folder": {
        "cmd": "cd \"{{folder}}\" && bash --login"
      }
    }
  },
  "targets": {}
}
